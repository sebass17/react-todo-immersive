{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "eslint:recommended"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "import/prefer-default-export": 0
    }
}